<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="OcUpload" table="oc_upload">
    <id name="uploadId" type="integer" column="upload_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="name" type="string" column="name" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="filename" type="string" column="filename" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="code" type="string" column="code" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="dateAdded" type="datetime" column="date_added" nullable="false"/>
  </entity>
</doctrine-mapping>
