<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="OcExtension" table="oc_extension">
    <id name="extensionId" type="integer" column="extension_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="type" type="string" column="type" length="32" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="code" type="string" column="code" length="32" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
