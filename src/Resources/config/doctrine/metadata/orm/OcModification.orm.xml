      </options>
    </field>
    <field name="code" type="string" column="code" length="64" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="author" type="string" column="author" length="64" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="version" type="string" column="version" length="32" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="link" type="string" column="link" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="xml" type="text" column="xml" length="16777215" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="status" type="boolean" column="status" nullable="false"/>
    <field name="dateAdded" type="datetime" column="date_added" nullable="false"/>
  </entity>
</doctrine-mapping>
