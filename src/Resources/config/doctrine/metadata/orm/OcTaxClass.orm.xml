<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="OcTaxClass" table="oc_tax_class">
    <id name="taxClassId" type="integer" column="tax_class_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="title" type="string" column="title" length="32" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="description" type="string" column="description" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="dateAdded" type="datetime" column="date_added" nullable="false"/>
    <field name="dateModified" type="datetime" column="date_modified" nullable="false"/>
  </entity>
</doctrine-mapping>
